      "type": "row"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "cellOptions": { "type": "auto" },
            "inspect": false
          },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }] }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "CPU Request" },
            "properties": [{ "id": "unit", "value": "short" }, { "id": "custom.width", "value": 100 }]
          },
          {
            "matcher": { "id": "byName", "options": "Usage (avg)" },
            "properties": [{ "id": "unit", "value": "short" }, { "id": "custom.width", "value": 100 }]
          },
          {
            "matcher": { "id": "byName", "options": "Usage (P95)" },
            "properties": [{ "id": "unit", "value": "short" }, { "id": "custom.width", "value": 100 }]
          },
          {
            "matcher": { "id": "byName", "options": "Recommended" },
            "properties": [{ "id": "unit", "value": "short" }, { "id": "custom.width", "value": 110 }]
          },
          {
            "matcher": { "id": "byName", "options": "Utilization %" },
            "properties": [
              { "id": "unit", "value": "percent" },
              { "id": "custom.width", "value": 100 },
              { "id": "custom.cellOptions", "value": { "mode": "gradient", "type": "gauge", "valueDisplayMode": "text" } },
              { "id": "min", "value": 0 },
              { "id": "max", "value": 100 },
              { "id": "thresholds", "value": { "mode": "absolute", "steps": [{ "color": "red", "value": null }, { "color": "yellow", "value": 30 }, { "color": "green", "value": 50 }, { "color": "yellow", "value": 80 }, { "color": "red", "value": 90 }] } }
            ]
          }
        ]
      },
      "gridPos": { "h": 10, "w": 24, "x": 0, "y": 6 },
      "id": 201,
      "options": {
        "cellHeight": "sm",
        "footer": { "countRows": false, "fields": "", "reducer": ["sum"], "show": false },
        "showHeader": true,
        "sortBy": [{ "desc": false, "displayName": "Utilization %" }]
      },
      "targets": [
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "sum(kube_pod_container_resource_requests{resource=\"cpu\",container!=\"POD\",namespace=~\"$namespace\"}) by (namespace, pod)",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "request"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "avg_over_time(sum(rate(container_cpu_usage_seconds_total{container!=\"POD\",container!=\"\",namespace=~\"$namespace\"}[5m])) by (namespace, pod)[24h:])",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "avg"
        },
        {
          "datasource": { "type": "prometheus", "uid": "${datasource}" },
          "expr": "quantile_over_time(0.95, sum(rate(container_cpu_usage_seconds_total{container!=\"POD\",container!=\"\",namespace=~\"$namespace\"}[5m])) by (namespace, pod)[24h:])",
          "format": "table",
          "instant": true,
          "legendFormat": "",
          "refId": "p95"
        }
      ],
      "title": "CPU Optimization Recommendations",
      "transformations": [
        { "id": "seriesToColumns", "options": { "byField": "pod" } },
        { "id": "organize", "options": {
          "excludeByName": { "Time": true, "Time 1": true, "Time 2": true, "Time 3": true, "namespace 1": true, "namespace 2": true },
          "renameByName": { "namespace": "Namespace", "pod": "Pod", "Value #request": "CPU Request", "Value #avg": "Usage (avg)", "Value #p95": "Usage (P95)" }
        }},
        { "id": "calculateField", "options": { "alias": "Recommended", "binary": { "left": "Usage (P95)", "operator": "*", "right": "1.2" }, "mode": "binary", "reduce": { "reducer": "sum" } } },
        { "id": "calculateField", "options": { "alias": "Utilization %", "binary": { "left": "Usage (avg)", "operator": "/", "right": "CPU Request" }, "mode": "binary", "reduce": { "reducer": "sum" } } },
        { "id": "calculateField", "options": { "alias": "Utilization %", "binary": { "left": "Utilization %", "operator": "*", "right": "100" }, "mode": "binary", "reduce": { "reducer": "sum" }, "replaceFields": true } }
      ],
      "type": "table"
    },
